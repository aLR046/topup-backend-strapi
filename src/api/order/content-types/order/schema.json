{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Orders"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "order_code": {
      "type": "string",
      "required": false
    },
    "user_game_id": {
      "type": "string",
      "required": true
    },
    "server": {
      "type": "string"
    },
    "nickname": {
      "type": "string"
    },
    "total_price": {
      "type": "decimal",
      "min": 0
    },
    "order_status": {
      "type": "enumeration",
      "default": "pending",
      "enum": [
        "pending",
        "paid",
        "processing",
        "success",
        "failed"
      ]
    },
    "game": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::game.game",
      "inversedBy": "orders"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "orders"
    }
  }
}
